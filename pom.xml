    <profiles>
        <profile>
            <id>release-sign-artifacts</id>
            <activation>
                <property>
                    <name>performRelease</name>
                    <value>true</value>
                </property>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-gpg-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>sign-artifacts</id>
                                <phase>verify</phase>
                                <goals>
                                    <goal>sign</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>jetty8x</id>
            <activation>
                <jdk>[1.6,1.8)</jdk>
            </activation>
            <properties>
                <cargo.containerId>jetty8x</cargo.containerId>
            </properties>
        </profile>
        <profile>
            <id>jetty9x</id>
            <activation>
                <jdk>[1.8,)</jdk>
            </activation>
            <properties>
                <cargo.containerId>jetty9x</cargo.containerId>
            </properties>
        </profile>

    </profiles>
